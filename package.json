{
  "name": "vuetensils",
  "version": "0.4.3",
  "description": "A tasty toolset for Vue.js",
  "author": "Austin Gil <austin@stegosource.com>",
  "license": "MIT",
  "main": "dist/vuetensils.umd.js",
  "module": "dist/vuetensils.esm.js",
  "unpkg": "dist/vuetensils.min.js",
  "files": [
    "/src",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stegosource/vuetensils.git"
  },
  "scripts": {
    "dev": "npm run docs:dev",
    "test": "echo TODO",
    "lint": "eslint --fix src",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuetensils.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuetensils.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuetensils.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "commit": "git add . && git commit",
    "deploy": "release-it"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@commitlint/prompt": "^8.2.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-plugin-vue": "^5.2.3",
    "husky": "^3.0.9",
    "minimist": "^1.2.0",
    "release-it": "^12.4.3",
    "rollup": "^1.26.3",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^5.1.2",
    "stylelint": "^11.1.1",
    "stylelint-config-sass-guidelines": "^6.1.0",
    "vue-docgen-api": "^3.26.0",
    "vuepress": "^1.2.0",
    "vuepress-plugin-docgen": "^1.2.9",
    "vuepress-plugin-live": "^1.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add"
    }
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "vue2",
    "web",
    "components",
    "ui",
    "library",
    "component library",
    "accessibility",
    "accessible",
    "a11y",
    "semantic",
    "alert",
    "async",
    "drawer",
    "dropdown",
    "img",
    "image",
    "lazy-load",
    "lazyload",
    "input",
    "textarea",
    "checkbox",
    "radio",
    "select",
    "option",
    "intersection observer",
    "modal",
    "dialogue",
    "tabs",
    "toggle",
    "collapse"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
