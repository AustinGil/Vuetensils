{
  "name": "vuetensils",
  "version": "1.0.4-beta",
  "description": "A tasty toolset for Vue.js",
  "author": "Austin Gil <austin@stegosource.com>",
  "license": "MIT",
  "main": "dist/vuetensils.umd.js",
  "module": "dist/vuetensils.esm.js",
  "unpkg": "dist/vuetensils.min.js",
  "files": [
    "/src",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stegosource/vuetensils.git"
  },
  "scripts": {
    "dev": "npm run styleguide",
    "test": "echo TODO",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuetensils.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuetensils.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuetensils.min.js",
    "styleguide": "vue-styleguidist server",
    "styleguide:build": "vue-styleguidist build",
    "release": "release-it"
  },
  "keywords": [
    "web",
    "components",
    "ui",
    "library",
    "vueJS",
    "vue2",
    "dialogue",
    "checkbox",
    "radio",
    "collapse",
    "dropdown",
    "select",
    "option",
    "input",
    "list",
    "nav",
    "modal"
  ],
  "dependencies": {
    "popper.js": "^1.14.7"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "minimist": "^1.2.0",
    "release-it": "^12.3.4",
    "rollup": "^1.1.0",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-filesize": "^6.0.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^4.3.2",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-sass-guidelines": "^5.3.0",
    "vue": "^2.6.10",
    "vue-loader": "^15.7.1",
    "vue-styleguidist": "^3.18.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.30.0"
  }
}
