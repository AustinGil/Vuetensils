{
  "name": "vuetensils",
  "version": "0.6.3",
  "description": "A tasty toolset for Vue.js",
  "author": "Austin Gil <austin@stegosource.com>",
  "license": "MIT",
  "main": "dist/vuetensils.umd.js",
  "module": "dist/vuetensils.esm.js",
  "unpkg": "dist/vuetensils.min.js",
  "files": [
    "/src",
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stegosource/vuetensils.git"
  },
  "scripts": {
    "validate": "npm run build && npm run docs:build && npm t && npm run lint && git add -A && explorer . & npm run dev",
    "dev": "npm run docs:dev",
    "test": "npm run test:unit",
    "test:unit": "jest --config jest.config.js --coverage --runInBand",
    "lint": "eslint --fix src",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vuetensils.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vuetensils.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vuetensils.min.js",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs",
    "commit": "git add . && git commit",
    "deploy": "release-it"
  },
  "ManifestComments": [
    "Pinned jest to 24.9.0. 25.1.0+ is broken on Windows. Waiting for issue #9459 to be resolved."
  ],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@commitlint/prompt": "^8.3.5",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@vue/test-utils": "^1.0.2",
    "@vuepress/plugin-google-analytics": "^1.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "babel-loader": "^8.1.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^4.2.5",
    "jest": "24.9.0",
    "jest-serializer-vue-tjw": "^3.14.0",
    "minimist": "^1.2.5",
    "release-it": "^13.6.0",
    "rollup": "^2.1.0",
    "rollup-plugin-buble": "^0.19.8",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-uglify-es": "0.0.1",
    "rollup-plugin-vue": "^5.1.6",
    "stylelint": "^13.2.1",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "vue-docgen-api": "^4.14.0",
    "vue-jest": "^3.0.5",
    "vue-router": "^3.1.6",
    "vuepress": "^1.4.0",
    "vuepress-plugin-docgen": "^1.2.9",
    "vuepress-plugin-live": "^1.4.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && git add"
    }
  },
  "keywords": [
    "vue",
    "vuejs",
    "vue.js",
    "vue2",
    "web",
    "components",
    "ui",
    "library",
    "component library",
    "accessibility",
    "accessible",
    "a11y",
    "semantic",
    "alert",
    "async",
    "drawer",
    "dropdown",
    "img",
    "image",
    "lazy-load",
    "lazyload",
    "input",
    "textarea",
    "checkbox",
    "radio",
    "select",
    "option",
    "intersection observer",
    "modal",
    "dialog",
    "dialogue",
    "tabs",
    "toggle",
    "collapse"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
